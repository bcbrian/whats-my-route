{
  "name": "search-engine",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "webpack-dev-server --inline --content-base public/ --history-api-fallback --host 0.0.0.0",
    "start:prod": "npm run build && node server.bundle.js",
    "watch:prod": "nodemon --ignore server.bundle.js --ignore public/ -x npm run start:prod",
    "build:client": "webpack",
    "build:server": "webpack --config webpack.server.config.js",
    "build": "npm run build:client && npm run build:server",
    "lint": "eslint .",
    "test": "./.scripts/test.sh",
    "test:client": "",
    "test:console": "",
    "build:css": "cp -R node_modules/font-awesome/fonts public/ && node-sass --include-path node_modules client/main.scss public/bundle.css",
    "watch:css": "nodemon -e scss -x npm run build:css",
    "watch:help": "nodemon --help",
    "watch:test:console": "nodemon -e js,jsx -x npm run test:console",
    "chimp": "chimp --ddp=http://localhost:3000 --watch --browser=phantomjs",
    "test:chimp": "chimp --ddp=http://localhost:3000 --browser=phantomjs",
    "watch:chimp:console": "npm run start & npm run chimp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/bcbrian/finance-app.git"
  },
  "author": "Brian Bartholomew",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/bcbrian/finance-app/issues"
  },
  "homepage": "https://gitlab.com/bcbrian/finance-app#README",
  "dependencies": {
    "apollo-client": "^0.4.20",
    "apollo-server": "^0.3.2",
    "body-parser": "^1.15.2",
    "bootstrap": "^4.0.0-alpha.2",
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "font-awesome": "^4.6.1",
    "graphql": "^0.7.1",
    "graphql-tag": "^0.1.14",
    "graphql-tools": "^0.7.2",
    "if-env": "^1.0.0",
    "lodash": "^4.13.1",
    "mongodb": "^2.2.10",
    "mongoose": "^4.6.3",
    "node-sass": "^3.4.2",
    "react": "^15.3.2",
    "react-addons-css-transition-group": "^15.3.2",
    "react-addons-pure-render-mixin": "^15.x",
    "react-apollo": "^0.5.8",
    "react-dom": "^15.3.2",
    "react-dropzone": "^3.5.0",
    "react-google-charts": "1.1.1",
    "react-mounter": "^1.1.0",
    "react-router": "^2.8.1",
    "scriptjs": "^2.5.8",
    "shortid": "^2.2.6"
  },
