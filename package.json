    "babel-core": "^6.17.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "chimp": "^0.31.1",
    "css-loader": "^0.25.0",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "graphiql": "^0.7.8",
    "http-server": "^0.9.0",
    "nodemon": "^1.9.1",
    "react-addons-test-utils": "^15.x",
    "sass-loader": "^4.0.2",
